{
	"name": "aether",
	"build": {
		"context": "..",
		"dockerfile": "Dockerfile"
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-azuretools.vscode-docker",
				"ms-python.python"
			],
			"settings": {
				"python.defaultInterpreterPath": "~/.cache/pypoetry/virtualenvs"
			}
		}
	},
	"postCreateCommand": "poetry install"
}
